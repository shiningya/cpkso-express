            li
              a(href='#brandX',data-toggle='enter',onclick='return false;') X
            li
              a(href='#brandY',data-toggle='enter',onclick='return false;') Y
            li
              a(href='#brandZ',data-toggle='enter',onclick='return false;') Z
            li
        #tabcontent.tab-content
          #brandA.tab-pane.brand-list.active
            mixin brands(brands)
              each brand in brands
                a(href='/cars/search/#{ids[0]}-#{brand.id}-#{ids[2]}-#{ids[3]}-#{ids[4]}-#{ids[5]}') #{brand.brand}
            +brands(brands.a)
          #brandB.tab-pane.brand-list
            +brands(brands.b)
          #brandC.tab-pane.brand-list
            +brands(brands.c)
          #brandD.tab-pane.brand-list
            +brands(brands.d)
          #brandE.tab-pane.brand-list
            +brands(brands.e)
          #brandF.tab-pane.brand-list
            +brands(brands.f)
          #brandG.tab-pane.brand-list
            +brands(brands.g)
          #brandH.tab-pane.brand-list
            +brands(brands.h)
          #brandI.tab-pane.brand-list
            +brands(brands.i)
          #brandJ.tab-pane.brand-list
            +brands(brands.j)
