          #brandO.tab-pane.brand-list
            +brands(brands.o)
          #brandP.tab-pane.brand-list
            +brands(brands.p)
          #brandQ.tab-pane.brand-list
            +brands(brands.q)
          #brandR.tab-pane.brand-list
            +brands(brands.r)
          #brandS.tab-pane.brand-list
            +brands(brands.s)
          #brandT.tab-pane.brand-list
            +brands(brands.t)
          #brandU.tab-pane.brand-list
            +brands(brands.u)
          #brandV.tab-pane.brand-list
            +brands(brands.v)
          #brandW.tab-pane.brand-list
            +brands(brands.w)
          #brandX.tab-pane.brand-list
            +brands(brands.x)
          #brandY.tab-pane.brand-list
            +brands(brands.y)
          #brandZ.tab-pane.brand-list
            +brands(brands.z)
      dl#categoryPrice.category-list.clearfix
        dt.title 价格：
        dd.category-item
          a.choose-all.choose-on(href='/cars/search/#{ids[0]}-#{ids[1]}-0-#{ids[3]}-#{ids[4]}-#{ids[5]}') 不限
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-1-#{ids[3]}-#{ids[4]}-#{ids[5]}') 10万以下
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-2-#{ids[3]}-#{ids[4]}-#{ids[5]}') 10-20万
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-3-#{ids[3]}-#{ids[4]}-#{ids[5]}') 20-30万
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-4-#{ids[3]}-#{ids[4]}-#{ids[5]}') 30-50万
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-5-#{ids[3]}-#{ids[4]}-#{ids[5]}') 50万以上
      dl#categoryMile.category-list.clearfix
        dt.title 续航：
        dd.category-item
          a.choose-all.choose-on(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-0-#{ids[4]}-#{ids[5]}') 不限
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-1-#{ids[4]}-#{ids[5]}') 100公里以下
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-2-#{ids[4]}-#{ids[5]}') 100-200公里
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-3-#{ids[4]}-#{ids[5]}') 200-300公里
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-4-#{ids[4]}-#{ids[5]}') 300-400公里
          a(href='/cars/search/#{ids[0]}-#{ids[1]}-#{ids[2]}-5-#{ids[4]}-#{ids[5]}') 400公里以上
      if ids[0] === '3'
        dl#categoryType.category-list.clearfix
          dt.title 类型：
          dd.category-item
            a.choose-all.choose-on(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-0-#{ids[5]}') 不限
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-纯电动-#{ids[5]}') 纯电动
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-插电式-#{ids[5]}') 插电式
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-增程式-#{ids[5]}') 增程式
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-混合动力-#{ids[5]}') 混合动力
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-燃料电池-#{ids[5]}') 燃料电池
        dl#categoryLevel.category-list.clearfix
          dt.title 级别：
          dd.category-item
            a.choose-all.choose-on(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-0') 不限
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-微型') 微型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-小型') 小型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-紧凑型') 紧凑型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-中型') 中型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-中大型') 中大型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-豪华型') 豪华型
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-SUV') SUV
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-MPV') MPV
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-跑车') 跑车
            a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-商用') 商用
    .content-container.car-search-result
      .content-left
        #car-result.search-result.product-container
          ul.list-content
            each car in cars
              li.list-item
                .figure
                  a(href='/cars/#{car.car.id}')
                    if car.car.prompt===1
                      span.recommend-icon
                    img(src='http://ev.cpkso.com/uploadPath/#{car.car.pic_thumbs[0]}')
                  a.car-logo(href='javascript:;')
                    if car.car.brand
                      img(src='http://ev.cpkso.com/brandPath/#{car.car.brand.pic}')
                  .car-info
                    .info-wrapper
                      a(href='/cars/#{car.car.id}') 详情
                      a.cus-service(href='javascript:;') 客服
                      a(href='/companys/#{car.car.company.id}',target='_blank') 官网
                    .car-detail
                      ul.detail-list
                        if car.car.params[0]&&car.car.params[0].mile
                          li
                            p 纯电续航：
                              span #{car.car.params[0].mile}km
                        if car.car.params[0]&&car.car.params[0].max_speed
                          li
                            p 最高时速：
                              span #{car.car.params[0].max_speed}km
                        if car.car.params[0]&&car.car.params[0].charge_time
                          li
                            p 充电时间：
                              span #{car.car.params[0].charge_time}h
                        if car.car.params[0]&&car.car.params[0].accelerate
                          li
                            p 百公里加速：
                              span #{car.car.params[0].accelerate}s
                .detail
                  if car.car.w1===1
                    a.desc(href='/cars/#{car.car.id}',style='text-indent:26px;') #{car.car.name}
                      span.hot-icon
                  else
                    a.desc(href='/cars/#{car.car.id}') #{car.car.name}
                  .company-detail
                    a.company-name(href='/company/#{car.car.company.id}',target='_blank') #{car.car.company.name}
                      if car.car.company.vip===1
                        span.approve-icon
                  .msg
                    if car.car.params[0]&&car.car.params[0].price
                      p.price ￥#{car.car.params[0].price}万
                    else
                      p.price 暂无价格
                    if car.hot
                      p.view-count 已浏览#{car.hot}
        .page-wrapper
          .pagination
      .content-right
        #showBar1.show-bar-sm
          mixin bar(bar)
            a(href='/cars/#{bar[0].(car).id}')
              img(src='http://ev.cpkso.com/adsPath/#{bar[0].pic}')
          +bar(promo1)
        .info-container
          ul.nav.nav-tabs
            li.active
              a(href='#car-news',data-toggle='tab') 行业新闻
            li
              a(href='#policy',data-toggle='tab') 政策法规
          .tab-content
            #car-news.tab-pane.active
              mixin news(newsdata)
                ul.msg-list
                  each news in newsdata
                    li.side-list-item
                      a(href='/news/#{news.article.content}') #{news.article.title}
                      span.date #{news.article.publish_time}
              +news(promo2)
            #policy.tab-pane
              +news(promo3)
        #showBar2.show-bar-sm
          +bar(promo4)
        .info-container
          ul.nav.nav-tabs
            li.active
              a(href='#guide',data-toggle='tab') 车辆导购
            li
              a(href='#tips',data-toggle='tab') 用车贴士
          .tab-content
            #guide.tab-pane.active
              +news(promo5)
            #tips.tab-pane
              +news(promo6)
        #showBar3.show-bar-sm
          +bar(promo6)
    .cut-line

append script
  script(src='http://ev.cpkso.com/style/js/lib/enter.js')
  script(src='http://ev.cpkso.com/style/js/lib/jquery.page.js')
  script.
    $('.pagination').Mpage({
      pageCount: #{pageinfo.maxPageNo},
      current: #{pageinfo.pageNo},
      mclick: function(p){
        //- console.log(p);
      }
    });

