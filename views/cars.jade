extends layout

block title
  title= '车型库、新能源电动车车型大全-产品快搜新能源电动车'
  meta(name="keywords", content="低速电动车、电动乘用车、电动三轮车、电动客车、新能源电动车分类、新能源电动车品牌、新能源电动车价格、新能源电动车续航里程")
  meta(name="description", content="产品快搜新能源电动车车型库为您提供各种型号的新能源电动车，囊括了的参数、型号、图片、价格、厂家等信息，是的专业平台，带给您最准确的信息。")
append link
  link(rel="stylesheet", href="http://ev.cpkso.com/style/css/searchresult.css")
block content
  .product-head-nav
    ol.breadcrumb
      span.location 当前位置：
      li
        a(href='/') 首页
      li.active 车型库
  .main-body
    #filter.category
      dl#category.category-list.clearfix
        dt.title 分类：
        .category-item
          a.choose-all.choose-on(href='/cars/search/0-#{ids[1]}-#{ids[2]}-#{ids[3]}-0-0') 不限
          a(href='/cars/search/15-#{ids[1]}-#{ids[2]}-#{ids[3]}-0-0') 低速电动车
          a(href='/cars/search/3-#{ids[1]}-#{ids[2]}-#{ids[3]}-#{ids[4]}-#{ids[5]}') 电动乘用车
          a(href='/cars/search/14-#{ids[1]}-#{ids[2]}-#{ids[3]}-0-0') 电动三轮车
          a(href='/cars/search/4-#{ids[1]}-#{ids[2]}-#{ids[3]}-0-0') 电动客车
      #categoryBrand.category-list.clearfix
        dt.title 品牌：
        a.choose-all.choose-on(href='/cars/search/#{ids[0]}-0-#{ids[2]}-#{ids[3]}-#{ids[4]}-#{ids[5]}') 不限
        #firstLetters.brand-ident
          ul
            li.active
              a(href='#brandA',data-toggle='enter',onclick='return false;') A
            li
              a(href='#brandB',data-toggle='enter',onclick='return false;') B
            li
              a(href='#brandC',data-toggle='enter',onclick='return false;') C
            li
              a(href='#brandD',data-toggle='enter',onclick='return false;') D
            li
              a(href='#brandE',data-toggle='enter',onclick='return false;') E
            li
              a(href='#brandF',data-toggle='enter',onclick='return false;') F
            li
              a(href='#brandG',data-toggle='enter',onclick='return false;') G
            li
              a(href='#brandH',data-toggle='enter',onclick='return false;') H
            li
              a(href='#brandI',data-toggle='enter',onclick='return false;') I
            li
              a(href='#brandJ',data-toggle='enter',onclick='return false;') J
            li
              a(href='#brandK',data-toggle='enter',onclick='return false;') K
            li
              a(href='#brandL',data-toggle='enter',onclick='return false;') L
