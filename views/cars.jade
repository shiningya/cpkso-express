              a(href='#brandM',data-toggle='enter',onclick='return false;') M
            li
              a(href='#brandN',data-toggle='enter',onclick='return false;') N
            li
              a(href='#brandO',data-toggle='enter',onclick='return false;') O
            li
              a(href='#brandP',data-toggle='enter',onclick='return false;') P
            li
