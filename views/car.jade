              $(carType[i]).attr('checked', true);
            }
            $("input.car-type").attr('disabled', true);
            if ($("input.car-type:checked").length >= 3) {
              $("input.car-type:checked").attr('disabled', false);
            } else {
              $("input.car-type").attr('disabled', false);
            };
