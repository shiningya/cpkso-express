extends layout

head
  block title
    title= '车型库、新能源电动车车型大全-产品快搜新能源电动车'
    meta(name="keywords", content="低速电动车、电动乘用车、电动三轮车、电动客车、新能源电动车分类、新能源电动车品牌、新能源电动车价格、新能源电动车续航里程")
    meta(name="description", content="产品快搜新能源电动车车型库为您提供各种型号的新能源电动车，囊括了的参数、型号、图片、价格、厂家等信息，是的专业平台，带给您最准确的信息。")
  append link
    link(rel="stylesheet", href="http://ev.cpkso.com/style/css/searchresult.css")
    link(rel="stylesheet", href="http://ev.cpkso.com/style/css/cardetail.css")
    link(rel="stylesheet", href="http://ev.cpkso.com/style/css/news.css")
    link(rel="stylesheet", href="http://ev.cpkso.com/style/css/owl.carousel.min.css")
    link(rel="stylesheet", href="http://ev.cpkso.com/style/css/param_detail.css")
body(data-spy='scroll',data-target='.myScrollspy')
  block content
    .product-head-nav
      ol.breadcrumb
        span.location 当前位置：
        li
            a(href='/') 首页
        li
            a(href='/cars/') 车型库
        li.active #{car.car.name}
    .main-body
      #pdetail.product-intro.clearfix
        .preview-wrapper
          .preview
            .main-img.picZoomer
              img(src='http://ev.cpkso.com/uploadPath/#{car.car.pics[0]}')
            .spec-list
              .spec-items.J-img-list
                ul#owl-sm.owl-carousel
                  each pic in car.car.pics
                    li.item
                      img(src='http://ev.cpkso.com/uploadPath/#{pic}')
        .item-info-wrapper
          .header-wrapper.clearfix
            .product-name #{car.car.name}
          .guide-price
            .dt 新车指导价：
              if car.car.params[0]&&car.car.params[0].price
                span.price ￥#{car.car.params[0].price}万
              else
                span.price 暂无价格
            .dd 已有
              span.view-count #{car.car.hot}
              人浏览
          .company.li
            .dt 公司
            .dd #{car.car.company.name}
              if car.car.company.vip===1
                span
          .choose-type.li.clearfix
            .dt.clearfix 车型配置
            #choose.dd
             ul.clearfix
              each param,index in car.car.params
                if index===0
                  li.selected
                    a(href='javascript:;') #{param.car_model}
                else
                  li
                    a(href='javascript:;') #{param.car_model}
          .contact.li
            .dt 联系人
            if car.car.company.charge
              .dd #{car.car.company.charge}
            else
              .dd 暂无
          .address.li
            .dt 地址
            if car.car.company.address
              .dd #{car.car.company.address}
            else
              .dd 暂无
          .service
            if car.car.company.url
              a.span.website(href='#{car.car.company.url}',rel='nofollow',target='_blank') 官网
            a.span.home(href='/companies/#{car.car.company.id}',target='_blank') 企业库
            if car.car.company.contact||car.car.company.contact2
              a.span.tel(href='javascript:;') 联系方式
                span #{car.car.company.contact}  #{car.car.company.contact2}
      .content-container
        .content-left
          .product-container
            ul.nav.nav-tabs
              li.active
                a(href='#product-detail',data-toggle='tab') 产品详情
              li
                a(href='#product-config',data-toggle='tab') 参数配置
            .tab-content
              #product-detail.tab-pane.active
                div!= car.car.detail
              #product-config.tab-pane                
                div(style='width:920px;')
                  form#filter.param-filter.wrap(action='http://ev.cpkso.com/ev/param_findParamTables',method='post',enctype='multipart/form-data')
                  .nav.select-wrap.wrap
                    .myScrollspy
