      function tableRender(tpl, data, el) {
        var data = data.response.data;
        var emptyParams = []
        if (data.Params === null) {
            data.Params = emptyParams;
        }

        var empty = {};
        while (data.Params.length < 3) {
            data.Params.push(empty);
        };
        for (var i = 0; i < data.Params.length; i++) {
            data.Params[i].color_name = data.Params[i].param_table ? data.Params[i].param_table.car.color_name : '';
            // data.Params[i].price = data.Param[i].param_table.price?'¥ '+data.Params[i].param_table.price+'万':'暂无价格'
        }
        var content = Mustache.render(tpl, data);
        el.html(content);
      }

      function tplRender(tpl, data, el) {
        var data = data.response.data;
        var content = Mustache.render(tpl, data);
        el.html(content);
      }
