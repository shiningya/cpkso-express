    .content-container.car-search-result
      .content-left
        ul#com-list
          each company in companies
            li.com
              .com-logo
                a(href='/companies/#{company.id}',title='#{company.name}')
                  if company.logo
                    img(src='#{company.logo}',alt='#{company.name}')
                  else
                    img(src='/img/com-logo.png',alt='#{company.name}')
              .com-detail
                h4
                  a.title(href='/companies/#{company.id}',title='#{company.name}') #{company.name}
                  if company.vip===1
                    span.approve-icon
                dl
                  dt.title 企业类型：
                  if company.company_ct==='0'
                    dd 生产型
                  else if company.company_ct==='1'
                    dd 销售型
                  else
                    dd #{company.company_ct}
                  dt.title 注册地址：
                  if company.province
                    dd #{company.province}
                  else
                    dd 暂无信息
                  dt.title 联系人：
                  if company.charge
                    dd #{company.charge}
                  else
                    dd 暂无信息
                .com-brief
                  span.title 企业简介：
                  span.brief #{company.brief}
              .com-site
                a(href='/companies/#{company.id}',target='_blank',title='#{company.name}') 企业官网
