            a(href='/companies/search/21-#{ids[1]}') 宁夏
          #brandQ.tab-pane.brand-list
            a(href='/companies/search/22-#{ids[1]}') 青海
          #brandS.tab-pane.brand-list
            a(href='/companies/search/23-#{ids[1]}') 山东
            a(href='/companies/search/24-#{ids[1]}') 山西
            a(href='/companies/search/25-#{ids[1]}') 陕西
            a(href='/companies/search/26-#{ids[1]}') 四川
            a(href='/companies/search/27-#{ids[1]}') 上海
          #brandT.tab-pane.brand-list
